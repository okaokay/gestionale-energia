{
  "name": "gestionale-energia",
  "version": "1.0.0",
  "description": "Gestionale completo per agenzia luce e gas con AI",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon --exec ts-node backend/server.ts",
    "client": "cd frontend && npm run dev",
    "build": "tsc && cd frontend && npm run build",
    "setup": "npm install && cd frontend && npm install",
    "db:migrate": "ts-node backend/database/migrate-sqlite.ts",
    "db:migrate-email": "ts-node backend/database/migrate-email-system.ts",
    "db:migrate-docs": "ts-node backend/database/migrate-documenti.ts",
    "db:migrate-clienti": "ts-node backend/database/migrate-clienti-completi.ts",
    "db:migrate-nullable": "ts-node backend/database/migrate-clienti-nullable.ts",
    "db:migrate-config": "ts-node backend/database/migrate-configurazioni.ts",
    "db:seed-emails": "ts-node backend/database/seed-email-logs.ts",
    "db:migrate-ai": "ts-node backend/database/migrate-ai-config.ts",
    "db:import-demo": "ts-node backend/database/import-demo-data.ts",
    "db:seed": "ts-node backend/database/seed-more-demo.ts",
    "start": "node dist/backend/server.js"
  },
  "dependencies": {
    "@types/json2csv": "^5.0.7",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^12.4.1",
    "bullmq": "^5.59.0",
    "canvas": "^3.2.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parse": "^6.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.8.0",
    "json2csv": "^6.0.0-alpha.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.10.1",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^1.1.1",
    "pdf2json": "^3.2.2",
    "pdfjs-dist": "^5.4.296",
    "pg": "^8.11.3",
    "sharp": "^0.34.4",
    "sqlite3": "^5.1.7",
    "tesseract.js": "^5.1.1",
    "uuid": "^13.0.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/better-sqlite3": "^7.6.13",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.10.4",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.20",
    "@types/pdf-parse": "^1.1.4",
    "@types/pg": "^8.15.5",
    "@types/xlsx": "^0.0.35",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
